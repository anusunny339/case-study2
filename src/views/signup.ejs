          <form  method="POST" action="/signup/add" class="box2" onsubmit="return check();">
            
            <h1>signup</h1>
            <table>
             <tr>
                 <td><label>First Name</label></td>
                 <td><label>Last Name</label></td>
             </tr>
             <tr>
                 <td><input type="text" class="form-control" id="fname" name="fname" required>
                   
                 <td><input type="text" class="form-control"  id="lname" name="lname" required>
                
             </tr>
             <tr><td><span id="nameerror"></span></td>
             <td><span id="lerror"></span></td></tr>
             <tr>
                 <td><label>Date Of Birth</label></td> 
                 <td><label>Email</label></td></tr>
               <tr>
               <td><input type="date" class="form-control" id="dob" name="dob" required></td>
               
               <td><input type="email" class="form-control" id="emails" name="emails" required>
                 </td>
           </tr>
           <tr><td><span id="doberror"></span></td>
             <td><span id="emailerror"></span></td></tr>
           
           <tr>
               <td><label>Contact No</label></td> 
                 <td><label>Address</label></td></tr> 
           </tr>
           <tr>
             <td><input type="text" class="form-control" id="phone" name="phone" required></td>
             
             <td><input type="text" class="form-control"  id="address" name="address" required>
               </td>
         </tr>

         <tr><td><span id="phoneerror"></span></td>
             <td><span id="addresserror"></span></td></tr>
         <tr>
           <td><label>Password</label></td>
           <td><label>Confirm Password</label></td>
       </tr>
      
       <tr>
         <td><input type="password" class="form-control" id="password" name="password" required>
           </td>
          
         <td><input type="password" class="form-control" id="cpassword" name="cpassword" required></td>
         </tr>
         
         
         <tr><td>
           <span id="passworderror"></span>
           <!---->
           <div id="myprogress">
             <div id="mybar">0%</div>
           </div>
           <!---->
         </td>
           <td><span id="cpassworderror"></span></td></tr>
     <tr>
       <td><input type="reset"  onclick="resets();" value="reset"></td>
       <td><button type="submit">submit</button></td>
     </tr>
           <tr>
             <td></td>
             <!--<progress max="100" value="0" id="strengthBar" style="width:230px;">
